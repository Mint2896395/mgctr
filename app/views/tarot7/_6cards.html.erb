function drag(ev) {
    var el = ev.target;
    var parent = el.getAttribute("data-parent");
    
    if(!parent){
        el.setAttribute("data-parent", el.parentNode.id);
    }
    
    ev.dataTransfer.setData("Text", el.id);
    
}

function drop_6_01(ev) {

    ev.preventDefault();
    var data = ev.dataTransfer.getData("Text");
    ev.target.appendChild(document.getElementById(data));
    document.getElementById(data).style.height = "100%";
    document.getElementById(data).style.width = "100%";
    document.getElementById(data).style.left = "0%";
    document.getElementById(data).style.top = "0%";
    document.getElementById(data).setAttribute('draggable', false);
    
    document.getElementById(data).setAttribute('data-toggle', "modal");
    document.getElementById("cardback_6_1").setAttribute('onclick', "return flip_6_1()");
    document.getElementById("carddrop_6_01").setAttribute('ondragleave', "");

    var abc =  document.getElementById(data);
    document.getElementById(data).src = "/<%= @name %>/mycard" + abc.id + ".jpg";
    document.getElementById(data).setAttribute('onclick', "compare(" + abc.id + ")" );

    document.getElementById("cardback_6_1").style.visibility = 'visible';
    document.getElementById("cardback_6_1").setAttribute('id', abc.id);
    $('.card_6_1').toggleClass('flipped');

    document.getElementById("cardf_6_01").value = abc.id;
    document.getElementById("cardimage_6_1").src = "/<%= @name %>/mycard" + abc.id + ".jpg";
    document.getElementById("cardback_6_1").src = "./<%= @name %>/CardBack.jpg";
    document.getElementById("cardimage_6_1").style.visibility = 'visible';
}


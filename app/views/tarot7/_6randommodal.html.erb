<script>
		function randomFromTo_6_(from, to){
			return Math.floor(Math.random() * (to - from + 1) + from);
		}
  
		function moveRandom_6_(obj) {
			/* get container position and size
			 * -- access method : cPos.top and cPos.left */
			var cPos = $('#container_6_').offset();
			var cHeight = $('#container_6_').height();
			var cWidth = $('#container_6_').width();
			
			// get box padding (assume all padding have same value)
			var pad = parseInt($('#container_6_').css('padding-top').replace('px', ''));
			
			// get movable box size
			var bHeight = obj.height();
			var bWidth = obj.width();
			
			// set maximum position
			maxY = cPos.top + cHeight - bHeight - pad;
			maxX = cPos.left + cWidth - bWidth - pad;
			
			// set minimum position
			minY = cPos.top + pad;
			minX = cPos.left + pad;
			
			// set new position			
			newY = randomFromTo_6_(minY, maxY);
			newX = randomFromTo_6_(minX, maxX);
			
			obj.animate({
				top: newY,
				left: newX
				}, 500, function() {
          moveRandom_6_(obj);
			});
		}
$('.latestblock_6_').each(function() {
moveRandom_6_($(this));
});

$("#container_6_").click(function(){    
    $(".latestblock_6_").stop();
    });

$("#randomimg_6_").click(function(){
  $('.latestblock_6_').each(function() {
  moveRandom_6_($(this));
  });

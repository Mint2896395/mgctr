<script>
		function randomFromTo_5_(from, to){
			return Math.floor(Math.random() * (to - from + 1) + from);
		}
  
		function moveRandom_5_(obj) {
			/* get container position and size
			 * -- access method : cPos.top and cPos.left */
			var cPos = $('#container_5_').offset();
			var cHeight = $('#container_5_').height();
			var cWidth = $('#container_5_').width();
			
			// get box padding (assume all padding have same value)
			var pad = parseInt($('#container_5_').css('padding-top').replace('px', ''));
			
			// get movable box size
			var bHeight = obj.height();
			var bWidth = obj.width();
			
			// set maximum position
			maxY = cPos.top + cHeight - bHeight - pad;
			maxX = cPos.left + cWidth - bWidth - pad;
			
			// set minimum position
			minY = cPos.top + pad;
			minX = cPos.left + pad;
			
			// set new position			
			newY = randomFromTo_5_(minY, maxY);
			newX = randomFromTo_5_(minX, maxX);
			
			obj.animate({
				top: newY,
				left: newX
				}, 500, function() {
          moveRandom_5_(obj);
			});
		}
$('.latestblock_5_').each(function() {
moveRandom_5_($(this));
});

$("#container_5_").click(function(){    
    $(".latestblock_5_").stop();
    });

$("#randomimg_5_").click(function(){
  $('.latestblock_5_').each(function() {
  moveRandom_5_($(this));
  });
});


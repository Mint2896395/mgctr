         $('.hideicon').css('visibility', 'visible');
        }
    });

    $("#selectized_12_").change(function () {
        $("#wrapper_12_").toggleClass("toggled");
        $('.lefthand').css('left', '1%');
        btn_left_12_=true; 
        var abcd = document.getElementById("selectized_12_").options[this.selectedIndex].innerHTML;
        document.getElementById("nameselect_12_").innerHTML = abcd;
    });

function beforeflip_12_() {
    if(btn_sword_12_) {
        flip_12_()
    }else{
        oneCard_12_()
        setTimeout(function(){
            flip_12_()
        }, 1000); 
        btn_sword_12_=true;
    }
}

var btn_sword_12_ = true;
function oneCard_12_(){
    if(btn_sword_12_) {
    $('#contentreload_12_').show().delay(1000).hide();
    $('#contentonecard_12_').hide().delay(1000).show();
    document.getElementById("contentonecard_12_").style.visibility = 'visible';
    btn_sword_12_=false;
    }else{
    $('#contentreload_12_').hide().delay(1000).show();
    $('#contentonecard_12_').show().delay(1000).hide();
   document.getElementById("contentonecard_12_").style.visibility = 'hidden';

    btn_sword_12_=true;
    }
}      
    </script>
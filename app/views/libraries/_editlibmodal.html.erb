<div class="modal fade" id="editlibModal" tabindex="-1" role="dialog" aria-labelledby="editlibModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="editModalLabel">Edit Library</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render 'libraries/form', library: @library %>
      </div>
    </div>
  </div>
</div>

 <!-- Menu Toggle Script -->
    <script>
    
    var btn_left_1_=true;
    $("#menu-toggle_1_").click(function(e) {
        e.preventDefault();
        $("#wrapper_1_").toggleClass("toggled");
        if(btn_left_1_) {
        $('.lefthand').css('left', '260px');         $('.hideicon').css('visibility', 'hidden');
        btn_left_1_=false;
        }else{
        btn_left_1_=true;
         $('.lefthand').css('left', '1%');          $('.hideicon').css('visibility', 'visible');
        }
    });

    $("#selectized_1_").change(function () {
        $("#wrapper_1_").toggleClass("toggled");
        $('.lefthand').css('left', '1%');
        btn_left_1_=true; 
        var abcd = document.getElementById("selectized_1_").options[this.selectedIndex].innerHTML;
        document.getElementById("nameselect_1_").innerHTML = abcd;
    });

    
    </script>
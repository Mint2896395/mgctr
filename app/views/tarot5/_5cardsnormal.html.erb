    var data = ev.dataTransfer.getData("Text");
    ev.target.appendChild(document.getElementById(data));
    document.getElementById(data).style.height = "100%";
    document.getElementById(data).style.width = "100%";
    document.getElementById(data).style.left = "0%";
    document.getElementById(data).style.top = "0%";
    document.getElementById(data).setAttribute('draggable', false);
    document.getElementById(data).setAttribute('onclick', "compare()");
    document.getElementById(data).setAttribute('data-toggle', "modal");
    document.getElementById("cardback_5_11").setAttribute('onclick', "return flip_5_11()");
    document.getElementById("carddrop_5_11").setAttribute('ondragleave', "");

    var abc =  document.getElementById(data);
    document.getElementById(data).src = "/<%= @name %>/mycard" + abc.id + ".jpg";
    document.getElementById(data).setAttribute('onclick', "compare(" + abc.id + ")" );
    

    document.getElementById("cardback_5_11").style.visibility = 'visible';
    document.getElementById("cardback_5_11").setAttribute('id', abc.id);
    $('.card_5_11').toggleClass('flipped');

    document.getElementById("cardf_5_11").value = abc.id;
    document.getElementById("cardimage_5_11").src = "/<%= @name %>/mycard" + abc.id + ".jpg";
    document.getElementById("cardback_5_11").src = "./<%= @name %>/CardBack.jpg";
    document.getElementById("cardimage_5_11").style.visibility = 'visible';
}

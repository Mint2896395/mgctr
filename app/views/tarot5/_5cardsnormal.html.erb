 <!-- Menu Toggle Script -->
    <script>
    
    var btn_left_5_=true;
    $("#menu-toggle_5_").click(function(e) {
        e.preventDefault();
        $("#wrapper_5_").toggleClass("toggled");
        if(btn_left_5_) {
        $('.lefthand').css('left', '260px');         $('.hideicon').css('visibility', 'hidden');
        btn_left_5_=false;
        }else{
        btn_left_5_=true;
         $('.lefthand').css('left', '1%');          $('.hideicon').css('visibility', 'visible');
        }
    });

    $("#selectized_5_").change(function () {
        $("#wrapper_5_").toggleClass("toggled");
        $('.lefthand').css('left', '1%');
        btn_left_5_=true; 
        var abcd = document.getElementById("selectized_5_").options[this.selectedIndex].innerHTML;
        document.getElementById("nameselect_5_").innerHTML = abcd;
    });

function beforeflip_5_() {
    if(btn_sword_5_) {
        flip_5_()
    }else{
        oneCard_5_()
        setTimeout(function(){
            flip_5_()
        }, 1000); 
        btn_sword_5_=true;
    }
}

var btn_sword_5_ = true;
function oneCard_5_(){
    if(btn_sword_5_) {
    $('#contentreload_5_').show().delay(1000).hide();
    $('#contentonecard_5_').hide().delay(1000).show();
    document.getElementById("contentonecard_5_").style.visibility = 'visible';
    btn_sword_5_=false;
    }else{
    $('#contentreload_5_').hide().delay(1000).show();
    $('#contentonecard_5_').show().delay(1000).hide();
   document.getElementById("contentonecard_5_").style.visibility = 'hidden';

    btn_sword_5_=true;
    }
}     
    </script>
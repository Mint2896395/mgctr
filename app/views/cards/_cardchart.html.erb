<% end %>
<% if notfirstpage1 == "person_id" || notfirstpage2 == "person_id"  %>
  <img id= "samedate" href="#" class="cbShuffle" alt="" src="../imagesxx/thor.png" style="top: 35%" ></img>
<% end %>

<section style="position: relative;
  width: 100%;
  overflow-x: scroll;
  white-space: nowrap;">
<div id="container" style="position:relative; z-index: 555; max-width: 100%; height: 35vh; left:0 margin: 0 auto; ">
  
  <% b = @cards1.maximum(:price).to_f %>
  <% a = @cards1.minimum(:price).to_f %>
  <% c = Setting.last.setting03  %>

  <% if notfirstpagex == "1"  %>
    <% @cardsc = @cardsc %>
    <% @cards = @cards1 %>
    <% k = 200000 %>
    <% kpx = (82 * @cardsc.count).to_s + "px" %>
  <% else %>
    <% k = 20 %>
    <% kpx = (82* @cardsc2.limit(k).count).to_s + "px" %>
  <% end %>

  <% if notfirstpage1 == "person_id" || notfirstpage2 == "person_id" || notfirstpage3 == "person_id" %>
    <%= line_chart @cardsc2.limit(k).order(:date).group(:person_id).group(:date).sum(:price), discrete: true, curve: false, legend: false, min: a-(a/10) , max: b, width: kpx, height: "33vh"  %>
  <% else %>
    <%#= @cards1.where.not(person_id: "1").order(:date) %>
    <%= line_chart @cardsc.order(:date).group(:person_id).group(:date).sum(:price), discrete: true, curve: false, legend: true, min: 0 , max: c, width: "99%", height: "33vh",legend: "bottom"  %>
    
  <% end %>
  
</div>



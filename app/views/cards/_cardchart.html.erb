<%#= request.path -> "/allreferred"
request.fullpath -> "/allreferred?&referred_name=maria" %>
<% notfirstpage = (request.original_url.split('/').last) %>
<% notfirstpage1 = (request.fullpath.split(/\W+/)[-2]) %>
<% notfirstpage2 = (request.fullpath.split(/\W+/)[2]) %>
<% notfirstpage3 = (request.fullpath.split(/\W+/)[-4]) %>
<% pagenum = (request.fullpath.split(/\W+/)[3]).to_i %>
<% notfirstpagex = (request.fullpath.split('=').last) %>
<% indipage = request.fullpath.split(/\W+/)[2] %>

<% if  indipage == "page" || notfirstpage == "card2" %>
  <% @cardsx = @cards1 %>
  <% @cards2 = @cards1 %>
  <% if  notfirstpage3 == "person_id" %>
    <% @cardsc2 = @cardsc2 %>

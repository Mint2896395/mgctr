<script>
		function randomFromTo_2_(from, to){
			return Math.floor(Math.random() * (to - from + 1) + from);
		}
  
		function moveRandom_2_(obj) {
			/* get container position and size
			 * -- access method : cPos.top and cPos.left */
			var cPos = $('#container_2_').offset();
			var cHeight = $('#container_2_').height();
			var cWidth = $('#container_2_').width();
			
			// get box padding (assume all padding have same value)
			var pad = parseInt($('#container_2_').css('padding-top').replace('px', ''));
			
			// get movable box size
			var bHeight = obj.height();
			var bWidth = obj.width();
			
			// set maximum position
			maxY = cPos.top + cHeight - bHeight - pad;
			maxX = cPos.left + cWidth - bWidth - pad;
			
			// set minimum position
			minY = cPos.top + pad;
			minX = cPos.left + pad;
			
			// set new position			
			newY = randomFromTo_2_(minY, maxY);
			newX = randomFromTo_2_(minX, maxX);
			
			obj.animate({
				top: newY,
				left: newX
				}, 500, function() {
          moveRandom_2_(obj);
			});
		}
$('.latestblock_2_').each(function() {
moveRandom_2_($(this));
});

$("#container_2_").click(function(){    
    $(".latestblock_2_").stop();
    });

$("#randomimg_2_").click(function(){
  $('.latestblock_2_').each(function() {
  moveRandom_2_($(this));
  });
});

$("img").error(function () { 

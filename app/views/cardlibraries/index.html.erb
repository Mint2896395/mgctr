<p id="notice"><%= notice %></p>

<h1>Cardlibraries</h1>

<table>
  <thead>
    <tr>
      <th>Cardid</th>
      <th>Name</th>
      <th>Description</th>
      <th>Cardno</th>
      <th>Value</th>
      <th>Library</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cardlibraries.each do |cardlibrary| %>
      <tr>
        <td><%= cardlibrary.cardid %></td>
        <td><%= cardlibrary.name %></td>
        <td><%= cardlibrary.description %></td>
        <td><%= cardlibrary.cardno %></td>
        <td><%= cardlibrary.value %></td>
        <td><%= cardlibrary.library.name %></td>
        <td><%= link_to 'Show', cardlibrary %></td>
        <td><%= link_to 'Edit', edit_cardlibrary_path(cardlibrary) %></td>
        <td><%= link_to 'Destroy', cardlibrary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cardlibrary', new_cardlibrary_path %>
